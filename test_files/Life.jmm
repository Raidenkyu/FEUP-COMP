    public boolean update() {
        int i;
        int cur;
        int neighN;
        boolean goodPop;
        int[] newfieldd;

        newfieldd = new int[fieldd.length];

        i = 0;
        while (i < fieldd.length) {
            cur = fieldd[i];
            neighN = this.getLiveNeighborN(i);


            // Live cell
            if (!(cur < 1)) {
                goodPop = this.ge(neighN,UNDERPOP_LIM) && this.le(neighN,OVERPOP_LIM);
                if (!goodPop) {
                    newfieldd[i] = 0;
                } else {
                    newfieldd[i] = fieldd[i];
                }
            }
            // Dead cell
            else {
                if (this.eq(neighN,REPRODUCE_NUM)) {
                    newfieldd[i] = 1;
                } else {
                    newfieldd[i] = fieldd[i];
                }
            }

            i = i + 1;
        }

        fieldd = newfieldd;
        return true;

    }

